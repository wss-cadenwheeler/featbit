{
  "Logging": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Npgsql.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "DbProvider": "Postgres",
  "MqProvider": "Postgres",
  "CacheProvider": "Redis",
  "MongoDb": {
    "ConnectionString": "mongodb://admin:password@mongodb:27017",
    "Database": "featbit"
  },
  "Postgres": {
    "ConnectionString": "Host=postgresql;Port=5432;Username=postgres;Password=please_change_me;Database=featbit",
    "Password": ""
  },
  "Kafka": {
    "Producer": {
      "bootstrap.servers": "kafka:9092",
      "linger.ms": "50"
    },
    "Consumer": {
      "group.id": "featbit-control-plane",
      "bootstrap.servers": "kafka:9092",
      "auto.offset.reset": "earliest",
      "enable.auto.commit": true,
      "auto.commit.interval.ms": "5000",
      "enable.auto.offset.store": false
    }
  },
  "Redis": {
    "ConnectionString": "redis:6379",
    "Password": "",
    "Instances": [
      "redis:6379"
    ]
  },
  "Api": {
    "ApiKey": ""
  }
}
